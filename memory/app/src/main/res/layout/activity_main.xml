        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/cell5"
        app:layout_constraintTop_toBottomOf="@id/cell3"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell10"
        app:layout_constraintEnd_toStartOf="@id/cell8"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cell4"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell11"
        app:layout_constraintEnd_toStartOf="@id/cell9"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/cell7"
        app:layout_constraintTop_toBottomOf="@id/cell5"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/cell8"
        app:layout_constraintTop_toBottomOf="@id/cell6"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

