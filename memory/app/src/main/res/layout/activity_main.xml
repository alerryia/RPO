<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@color/colorBackground">

    <ImageButton
        android:id="@+id/cell1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell4"
        app:layout_constraintEnd_toStartOf="@id/cell2"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell5"
        app:layout_constraintEnd_toStartOf="@id/cell3"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/cell1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/cell2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell7"
        app:layout_constraintEnd_toStartOf="@id/cell5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cell1"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/none" />

    <ImageButton
        android:id="@+id/cell5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="0dp"
        android:background="@color/colorBackground"
        app:layout_constraintBottom_toTopOf="@id/cell8"
        app:layout_constraintEnd_toStartOf="@id/cell6"
        app:layout_constraintHorizontal_chainStyle="packed"
